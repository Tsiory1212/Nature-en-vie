{% extends 'base_admin.html.twig' %}

{% block title %}
    Admin | Ajout rédacteur 
{% endblock %}

{% block stylesheets %}
{% endblock %}

{% block body %}
	<div class="container mt-5">
		<h2 class="text-center">Ajout d'un rédacteur </h2>
				{{ form_start(formEditor)}}
					{{ form_row(formEditor.firstname)}}
					{{ form_row(formEditor.lastname)}}
					{{ form_row(formEditor.email)}}
					{{ form_row(formEditor.phone)}}
					{# {{ form_row(formEditor.adress)}} #}
					{{ form_row(formEditor.password)}}

						<button class="btn btn-success" type="submit"> Enregistrer</button>
				{{ form_end(formEditor)}}
			</div>
		</div>
	</div>
{% endblock %}
